{
  "name": "dslink-java-empty",
  "version": "0.0.1",
  "description": "Empty Java DSLink",
  "license": "Apache",
  "author": {
    "name": "Julian Villalva",
    "email": "Julian.Villalva@AcuityBrands.com"
  },
  "main": "bin/dslink-java-empty",
  "repository": {
    "type": "git",
    "url": "https://github.com/example6/dslink-java-empty"
  },
  "bugs": {
    "url": "https://github.com/example6/dslink-java-empty/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "EmptyDSLink"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.empty.EmptyDSLink"
    }
  }
}
